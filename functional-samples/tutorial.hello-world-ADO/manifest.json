{
  "name": "ADO hello world",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "hello.html",
    "default_icon": "hello_extensions.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://dev.azure.com/*"
      ]
    }
  ],
  "host_permissions": [
    "https://dev.azure.com/*"
  ],
  "permissions": [
    "tabGroups"
  ]
}
